{
  "name": "custom-elements-module-federation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:all": "npm run build:shell && npm run build:mf1 && npm run build:mf2",
    "build:shell": "webpack --config webpack.shell.config.js",
    "build:mf1": "webpack --config webpack.mf1.config.js",
    "build:mf2": "webpack --config webpack.mf2.config.js",
    "serve:all": "concurrently \"npm run serve:dist:shell\" \"npm run serve:dist:mf1\" \"npm run serve:dist:mf2\"",
    "serve:dist:shell": "serve ./dist/shell -l 5000",
    "serve:dist:mf1": "serve ./dist/mf1 -l 4200",
    "serve:dist:mf2": "serve ./dist/mf2 -l 4300",
    "serve:dev:all": "concurrently \"webpack-dev-server --config webpack.mf1.config.js\" \"webpack-dev-server --config webpack.shell.config.js\""
  },
  "author": "",
  "license" : "ISC",
  "devDependencies": {
    "concurrently": "^5.1.0",
    "css-loader": "^3.5.1",
    "html-webpack-plugin": "git://github.com/ScriptedAlchemy/html-webpack-plugin.git#master",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^4.0.0",
    "serve": "^11.3.0",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "git://github.com/webpack/webpack.git#dev-1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "rxjs": "^7.0.0-beta.0"
  }
}
